{
  "name": "@ava/typescript",
  "version": "3.0.1",
  "description": "TypeScript provider for AVA",
  "engines": {
    "node": ">=14.19 <15 || >=16.15 <17 || >=18"
  },
  "files": [
    "index.js"
  ],
  "exports": {
    ".": "./index.js"
  },
  "type": "module",
  "author": "Mark Wubben (https://novemberborn.net)",
  "repository": "avajs/typescript",
  "license": "MIT",
  "keywords": [
    "ava",
    "typescript"
  ],
  "scripts": {
    "test": "xo && c8 ava"
  },
  "dependencies": {
    "escape-string-regexp": "^5.0.0",
    "execa": "^5.1.1"
  },
  "devDependencies": {
    "ava": "4.0.0-rc.1",
    "c8": "^7.10.0",
    "del": "^6.0.0",
    "typescript": "^4.4.4",
    "xo": "^0.46.3"
  },
  "c8": {
    "reporter": [
      "html",
      "lcov",
      "text"
    ]
  },
  "ava": {
    "files": [
      "!test/broken-fixtures/**"
    ],
    "ignoredByWatcher": [
      "test/fixtures/**",
      "test/broken-fixtures/**"
    ],
    "timeout": "60s"
  },
  "xo": {
    "ignores": [
      "test/broken-fixtures",
      "test/fixtures/**/compiled/**"
    ]
  }
}
