{
  "name": "@ava/typescript",
  "version": "4.1.0",
  "description": "TypeScript provider for AVA",
  "engines": {
    "node": "^14.19 || ^16.15 || ^18 || ^20"
  },
  "files": [
    "index.js"
  ],
  "exports": {
    ".": "./index.js"
  },
  "type": "module",
  "author": "Mark Wubben (https://novemberborn.net)",
  "repository": "avajs/typescript",
  "license": "MIT",
  "keywords": [
    "ava",
    "typescript"
  ],
  "scripts": {
    "test": "xo && c8 ava"
  },
  "dependencies": {
    "escape-string-regexp": "^5.0.0",
    "execa": "^7.1.1"
  },
  "devDependencies": {
    "ava": "^5.3.1",
    "c8": "^8.0.0",
    "del": "^7.0.0",
    "typescript": "^5.1.3",
    "xo": "^0.54.2"
  },
  "c8": {
    "reporter": [
      "html",
      "lcov",
      "text"
    ]
  },
  "ava": {
    "files": [
      "!test/broken-fixtures/**"
    ],
    "ignoredByWatcher": [
      "test/fixtures/**",
      "test/broken-fixtures/**"
    ],
    "timeout": "60s"
  },
  "xo": {
    "ignores": [
      "test/broken-fixtures",
      "test/fixtures/**/compiled/**"
    ]
  }
}
